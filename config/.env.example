# Azure OpenAI Configuration
# Copy this file to .env and fill in your credentials

# Required for Azure OpenAI integration
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_KEY=your-api-key-here

# Optional: Specify deployment name (defaults to gpt-4)
AZURE_OPENAI_DEPLOYMENT=gpt-4

# Optional: API version (defaults to 2024-02-15-preview)
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Pipeline Configuration
# Optional: Custom storage directory (defaults to 'generated')
STORAGE_DIR=generated

# Optional: Enable debug mode
DEBUG=false
